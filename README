
# Build a UI for the Web Application
 pytest tests/test_predict.py -v -s
# Run app in docker
docker build -t abalone-age-prediction-fastapi -f Dockerfile .
docker run -d --name abalone-container-v1 -p 8082:80 abalone-age-prediction-fastapi

# Run pytest in docker
 docker build -t tests -f Dockerfile_pytest .
 docker run tests